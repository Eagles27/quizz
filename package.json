    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1",
    "react-toastify": "^9.1.3",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/redux-logger": "^3.0.9",
    "@types/redux-persist": "^4.3.1",
